programa {
  funcao inicio() {
    //Declarações de variáveis
    real valor, desconto




    //Solicitando dados.

    escreva("Digite o valor: ")
    leia(valor)

    escreva("Digite o valor do desconto: ")
    leia(desconto)


    //Calculando.
    desconto = valor * 10 / 100


    //Exibindo resultados.
    escreva("\nDesconto: " + desconto)



  }
}
